{
  "name": "rune-maker",
  "version": "1.1.2",
  "description": "A league of Legends assistant.",
  "main": "./dist/main.js",
  "homepage": "./",
  "scripts": {
    "react-start": "npx tailwindcss -i ./input.css -o ./public/style.css && react-scripts start",
    "react-build": "npx tailwindcss -i ./input.css -o ./public/style.css && react-scripts build",
    "react-test": "react-scripts test",
    "test:react": "react-scripts test",
    "test": "npx jest --watch",
    "react-eject": "react-scripts eject",
    "build": "react-scripts build && tsc",
    "watch": "tsc -w",
    "start": "npx tailwindcss -i ./input.css -o ./public/style.css && react-scripts build && tsc && electron-forge start",
    "package": "npx tailwindcss -i ./input.css -o ./public/style.css && react-scripts build && tsc && electron-forge package",
    "make": "npx tailwindcss -i ./input.css -o ./public/style.css && react-scripts build && tsc && electron-forge make",
    "tailwind": "npx tailwindcss -i ./input.css -o ./public/style.css --watch"
  },
  "author": "Igor Tiburcio Cavalcanti (duduisonfire)",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/axios": "^0.14.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^27.5.2",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^16.18.10",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-query": "^1.2.9",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "electron": "^22.3.27",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.8",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.29.4",
    "react-async": "^10.0.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.4.4",
    "styled-components": "^5.3.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
